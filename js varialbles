var Name = "DIVYA";
console.log("My Name: " + Name);
console.log(`Datatype of Name: ${typeof Name}`);
let age = 20;
console.log("My Age: " + age);
console.log(`Datatype of Age: ${typeof age}`);
const student = true;
console.log("I'm a student: " + student);
console.log(`Datatype of student: ${typeof student}`);
var City = "VIZAG";
console.log("My City: " + City);
console.log(`Datatype of City: ${typeof City}`);
var value = "NULL";
console.log("Value: " + value);
console.log(`Datatype of value: ${typeof value}`);
var balance = null;
console.log("My Balance: " + balance);
console.log(`Datatype of balance: ${typeof balance}`);
var pi = 3.14159;
console.log("Pi: " + pi);
console.log(`Datatype of pi: ${typeof pi}`);
var greeting = "Hello World";
console.log("Greeting: " + greeting);
console.log(`Datatype of greeting: ${typeof greeting}`);
var isSunny = true;
console.log("Is Sunny: " + isSunny);
console.log(`Datatype of isSunny: ${typeof isSunny}`);
var num = "5";
console.log("Favorite Number: " + num);
console.log(`Datatype of num: ${typeof num}`);
var mixedValue = "di12vy4a";
console.log("Mixed Value: " + mixedValue);
console.log(`Datatype of mixedValue: ${typeof mixedValue}`);
